---
import Footer from "../components/Footer.astro";

interface Props {
  title: string;
}
const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <meta
      name="description"
      content="Sajay Prakash is a junior Computer Science undergraduate at Rajalakshmi Engineering College, Chennai. These are links to his social media and websites"
    />
    <title>{title}</title>
  </head>
  <body class="min-h-screen bg-neutral-900 text-neutral-100">
    <slot />
    <Footer />
  </body>
</html>
